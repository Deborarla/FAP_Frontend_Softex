#pacote RH
package net.plataforma.rh;

public class dp
{
    public static void main(String[] args) {
        String nome;
        String pis;
        Double salario;
    }
    public String getNome(){
        return nome;
    }
        public String getpis(){
        return pis;
    }
        public String getsalario(){
        return salario;
    }
       public String setNome(){
        return nome;
    }
        public String setpis(){
        return pis;
    }
        public String setsalario(){
        return salario;
    }
    
    
}
#Pacote compras

